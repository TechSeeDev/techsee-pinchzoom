{
  "name": "@techsee/pinch-zoom-js",
  "version": "2.3.6",
  "description": "Browser library for multi-touch gestures to zoom and drag on any DOM element.",
  "author": "Manuel Stofer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechSeeDev/techsee-pinchzoom.git"
  },
  "keywords": [
    "pinchzoom",
    "pinch",
    "zoom",
    "gesture",
    "touch",
    "drag"
  ],
  "main": "dist/pinch-zoom.umd.js",
  "module": "src/pinch-zoom.js",
  "files": [
    "dist/pinch-zoom.min.js",
    "dist/pinch-zoom.umd.js",
    "dist/pinch-zoom.umd.min.js",
    "src/pinch-zoom.js",
    "src/pinch-zoom.d.ts"
  ],
  "typings": "src/pinch-zoom.d.ts",
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-minify": "^0.5.0",
    "babel-preset-env": "~1.6.1",
    "audit-ci": "^7.0.0",
    "gitleaks-secret-scanner": "latest",
    "lockfile-lint": "^4.13.0",
    "npm-package-json-lint": "^8.0.0",
    "@nodesecure/cli": "^3.0.0"
  },
  "scripts": {
    "build": "script/build.sh",
    "prepublish": "npm run build",
    "preinstall": "lockfile-lint --path package-lock.json --type npm --allowed-hosts npm registry.npmjs.org --validate-https && audit-ci --config .audit-ci.json && gitleaks-secret-scanner detect --no-git --source . --baseline-path .gitleaks-baseline.json && npmPkgJsonLint .",
    "audit:check": "audit-ci --config .audit-ci.json",
    "secrets:check": "gitleaks-secret-scanner detect --no-git --source . --baseline-path .gitleaks-baseline.json",
    "security:nodesecure": "nsecure auto"
  }
}
